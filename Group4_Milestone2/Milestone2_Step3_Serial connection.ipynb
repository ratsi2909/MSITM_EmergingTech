   "source": [
    "import serial \n",
    "s = serial.Serial(timeout=0.1) \n",
    "s.baudrate = 115200 \n",
    "s.port = '/dev/ttyACM0'\n",
    "s.open() \n",
    "while True:\n",
    "    data =str(s.readline())\n",
    "    if data: \n",
    "       print(data)\n",
    "    else:\n",
    "       print('No data')\n",
    "       break\n",
    "    \n",
    "s.close()"
   ]
  }
